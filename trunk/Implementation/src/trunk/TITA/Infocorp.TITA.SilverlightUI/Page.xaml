        		<Setter.Value>
        			<ControlTemplate TargetType="Infocorp_TITA_Controls_Silverlight_V2:Group">
        				<Grid Height="Auto" x:Name="RootElement" Width="Auto">
        					<Grid.Resources>
        						<Storyboard x:Name="sbOpen">
        							<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="scrollViewer" Storyboard.TargetProperty="(FrameworkElement.Height)">
